<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shailesh.youcanalsocook" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".ParseApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Signup"
            android:label="@string/title_activity_signup" >
        </activity>
        <activity
            android:name=".SearchResult"
            android:label="@string/title_activity_search_result" >
        </activity>
        <activity
            android:name=".ViewResult"
            android:label="@string/title_activity_view_result" >
        </activity>
        <activity
            android:name=".Spice_Vegetable_CheckBoxes"
            android:label="@string/title_activity_spice__vegetable__check_boxes" >
        </activity>
        <activity
            android:name=".ResetPassword"
            android:label="@string/title_activity_reset_password" >
        </activity>
    </application>

</manifest>
